    "@vue/test-utils": "2.4.6",
    "@vue/vue3-jest": "29.2.6",
    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "9.27.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
